{
    "name": "til-vscode",
    "displayName": "til-vscode",
    "description": "",
    "version": "0.0.1",
    "publisher": "rahuldhawani",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:til.activiate"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "til.activiate",
                "title": "TIL"
            }
        ],
        "keybindings": [
            {
                "command": "til.activiate",
                "key": "shift+alt+t",
                "mac": "shift+alt+t",
                "when": "editorFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "dev": "webpack --mode development --config ./webpack.config.js",
        "build": "webpack --mode production --config ./webpack.config.js && npm run compile"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-beta.52",
        "@babel/plugin-proposal-class-properties": "^7.0.0-beta.52",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.56",
        "@babel/preset-env": "^7.0.0-beta.52",
        "@babel/preset-react": "^7.0.0-beta.52",
        "@babel/preset-typescript": "^7.0.0-beta.52",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "@types/react": "^16.4.6",
        "@types/react-dom": "^16.0.6",
        "@types/slate": "^0.33.4",
        "babel-loader": "^8.0.0-beta",
        "css-loader": "^1.0.0",
        "mini-css-extract-plugin": "^0.4.1",
        "node-sass": "^4.9.3",
        "raw-loader": "^0.5.1",
        "sass-loader": "^7.1.0",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "webpack": "^4.15.1",
        "webpack-assets-manifest": "^3.0.1",
        "webpack-cli": "^3.0.8"
    },
    "dependencies": {
        "antd": "^3.9.2",
        "markup-it": "^9.0.0",
        "prismjs": "^1.13.0",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-textarea-autosize": "^7.0.4",
        "vscode-cache": "^0.3.0"
    }
}
